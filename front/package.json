{
    "name": "rotator_esp_web",
    "version": "0.4.2",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build && npm run gzip",
        "lint": "vue-cli-service lint",
        "fix": "eslint ./src/* --fix  && npm run fix2",
        "fix2": "eslint ./src/components/* --fix",
        "gzip": "cd dist/js && sudo 7z a -tgzip app.js.gz app.js && rm -rf app.js"
    },
    "dependencies": {
        "@material-design-icons/svg": "^0.13.0",
        "@mdi/js": "^7.0.96",
        "@yeliulee/vue-mdi-svg": "^1.1.0",
        "axios": "^1.1.2",
        "core-js": "^2.6.5",
        "vue": "^2.6.10",
        "vue-router": "^3.0.3",
        "vuetify": "^1.5.14",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@mdi/font": "^7.0.96",
        "@svgv/webpack": "^0.1.0",
        "@vue/cli-plugin-babel": "~5.0.0",
        "@vue/cli-plugin-eslint": "~5.0.0",
        "@vue/cli-service": "~5.0.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^8.33.0",
        "eslint-plugin-vue": "^9.6.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.1",
        "vue-cli-plugin-vuetify": "^0.5.0",
        "vue-template-compiler": "^2.5.21",
        "vuetify-loader": "^1.0.5"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}